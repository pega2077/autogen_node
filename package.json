{
  "name": "autogen_node",
  "version": "0.1.0",
  "description": "Node.js version of microsoft/autogen - A framework for building multi-agent AI systems",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/examples/basic-chat.ts",
    "example:basic": "ts-node src/examples/basic-chat.ts",
    "example:auto": "ts-node src/examples/automated-chat.ts",
    "example:group": "ts-node src/examples/group-chat.ts",
    "example:openrouter": "ts-node src/examples/openrouter-example.ts",
    "example:ollama": "ts-node src/examples/ollama-example.ts",
    "example:ollama-organizer": "ts-node src/examples/ollama-file-organizer-example.ts",
    "example:anthropic": "ts-node src/examples/anthropic-example.ts",
    "example:gemini": "ts-node src/examples/gemini-example.ts",
    "example:functions": "ts-node src/examples/function-calling-example.ts",
    "example:code": "ts-node src/examples/code-execution-example.ts",
    "example:memory": "ts-node src/examples/memory-example.ts",
    "example:streaming": "ts-node src/examples/streaming-example.ts",
    "example:nested": "ts-node src/examples/nested-chat-example.ts",
    "example:sequential": "ts-node src/examples/sequential-chat-example.ts",
    "example:speaker": "ts-node src/examples/speaker-selection-example.ts",
    "example:swarm": "ts-node src/examples/swarm-mode-example.ts",
    "example:context": "ts-node src/examples/context-management-example.ts",
    "example:events": "ts-node src/examples/event-driven-example.ts",
    "example:conversable": "ts-node src/examples/conversable-agent-example.ts",
    "example:teachable": "ts-node src/examples/teachable-agent-example.ts",
    "example:rag": "ts-node src/examples/rag-example.ts",
    "example:filesystem": "ts-node src/examples/filesystem-tool-example.ts",
    "example:browser": "ts-node src/examples/browser-tool-example.ts",
    "example:api": "ts-node src/examples/api-tool-example.ts",
    "example:docker": "ts-node src/examples/docker-executor-example.ts",
    "example:image": "ts-node src/examples/image-generation-example.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "autogen",
    "ai",
    "agent",
    "multi-agent",
    "openai",
    "gpt",
    "chatbot",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@google/generative-ai": "^0.24.1",
    "@types/dockerode": "^3.3.46",
    "axios": "^1.13.2",
    "dockerode": "^4.0.9",
    "dotenv": "^17.2.3",
    "openai": "^6.8.1",
    "playwright": "^1.56.1"
  }
}
